{
    "name": "Resume AI HackAI",
    "version": "1.0.0",
    "description": "This extension utilitzes AI to analyze your resume, create a set of potential job titles",
     "manifest_version": 2,
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "author": "hackAI group 1000",
  "action": {
    "default_popup": "index.html",
    "default_title": "ResumeAI HackAI"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "Resume AI HackAI",
    "default_icon": "icon.png"
  }
}
